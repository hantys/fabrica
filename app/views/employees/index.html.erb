<p id="notice"><%= notice %></p>

<h1>Employees</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Cpf</th>
      <th>Email</th>
      <th>Office</th>
      <th>Bank</th>
      <th>Ag</th>
      <th>Cc</th>
      <th>Variation</th>
      <th>Street</th>
      <th>Number</th>
      <th>String</th>
      <th>Neighborhood</th>
      <th>Cep</th>
      <th>State</th>
      <th>City</th>
      <th>Phone1</th>
      <th>Phone2</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @employees.each do |employee| %>
      <tr>
        <td><%= employee.name %></td>
        <td><%= employee.cpf %></td>
        <td><%= employee.email %></td>
        <td><%= employee.office %></td>
        <td><%= employee.bank %></td>
        <td><%= employee.ag %></td>
        <td><%= employee.cc %></td>
        <td><%= employee.variation %></td>
        <td><%= employee.street %></td>
        <td><%= employee.number %></td>
        <td><%= employee.string %></td>
        <td><%= employee.neighborhood %></td>
        <td><%= employee.cep %></td>
        <td><%= employee.state %></td>
        <td><%= employee.city %></td>
        <td><%= employee.phone1 %></td>
        <td><%= employee.phone2 %></td>
        <td><%= employee.user %></td>
        <td><%= link_to 'Show', employee %></td>
        <td><%= link_to 'Edit', edit_employee_path(employee) %></td>
        <td><%= link_to 'Destroy', employee, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Employee', new_employee_path %>
